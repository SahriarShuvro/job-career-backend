<!-- Job Add Modal  -->
<input type="checkbox" id="my-modal-6" class="modal-toggle" />
<div class="modal modal-bottom sm:modal-middle">
  <div
    class="alert alert-error shadow-lg fixed top-16 w-auto z-10 opacity-0 duration-500 overflow-hidden errAlartSection"
  >
    <div>
      <span>Sorry! Please fill in all the form inputs.</span>
    </div>
  </div>
  <div
    class="alert alert-success shadow-lg fixed top-16 w-auto z-10 opacity-0 duration-500 overflow-hidden successAlartSection"
  >
    <div>
      <span>Congratulations! Successfully added to blog post.</span>
    </div>
  </div>
  <div class="modal-box h-full flex flex-col justify-between scrollbar-hide">
    <div>
      <h3 class="font-bold text-lg mb-4">Add New Blog Post!</h3>
      <div>
        <div class="flex justify-between space-x-1">
          <form action="form" id="formID" class="space-y-4 w-full">
            <div class="jobTitle">
              <label
                for="jobTitle"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Blog Title</label
              >
              <input
                type="text"
                id="jobTitle"
                class="formInput bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-green-500 focus:border-green-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-green-500 dark:focus:border-green-500"
                placeholder="example"
                required
              />
            </div>

            <div class="category w-full">
              <label
                for="category"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Category</label
              >
              <select
                name="category"
                id="category"
                class="formInput w-full text-sm rounded-lg dark:bg-gray-700 text-gray-700 dark:text-gray-200 outline-none cursor-pointer focus:ring-green-500 bg-gray-50 border-gray-200"
              >
                <option value="">--Select Category--</option>
                <option value="Maneger">Maneger</option>
                <option value="Junior Maneger">Junior Maneger</option>
              </select>
            </div>

            <div class="detailsOne">
              <label
                for="detailsOne"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Details One</label
              >
              <textarea
                type="text"
                id="detailsOne"
                rows="3"
                name="detailsOne"
                class="formInput bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-green-500 focus:border-green-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-green-500 dark:focus:border-green-500 scrollbar-hide"
                placeholder="Put your first details here.."
                required
              ></textarea>
            </div>

            <div class="innerTitle">
              <label
                for="innerTitle"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Inner Title</label
              >
              <input
                type="text"
                id="innerTitle"
                name="innerTitle"
                class="formInput bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-green-500 focus:border-green-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-green-500 dark:focus:border-green-500"
                placeholder="example"
                required
              />
            </div>

            <div class="detailsTwo">
              <label
                for="detailsTwo"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Details Two</label
              >
              <textarea
                type="text"
                id="detailsTwo"
                rows="3"
                name="detailsTwo"
                class="formInput bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-green-500 focus:border-green-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-green-500 dark:focus:border-green-500 scrollbar-hide"
                placeholder="Put your details two here.."
                required
              ></textarea>
            </div>

            <div class="quotation">
              <label
                for="quotation"
                class="block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                >Quotation</label
              >
              <textarea
                type="text"
                id="quotation"
